{
    "name": "qunar schema qrcode",
    "manifest_version": 2,
    "version": "0.0.1",
    "description": "generates qrcode",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "page_action": {
        "default_icon": "icon.png",
        "default_title": "QunarQRCode",
        "default_popup": "index.html"
    },
    "permissions": [
        "http://*/",
        "https://*/",
        "tabs"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
